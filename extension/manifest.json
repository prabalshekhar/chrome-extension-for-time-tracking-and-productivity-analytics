{
    "manifest_version": 3,
    "name": "Time Tracker",
    "version": "1.0",
    "description": "Tracks time spent on websites and shows productivity analytics",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "host_permissions": ["<all_urls>"]
  }
  